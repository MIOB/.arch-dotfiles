URxvt*font: xft:JetBrainsMono Nerd Font:size=12
URxvt*boldFont: xft:JetBrainsMono Nerd Font:style=Bold:size=12
URxvt*italicFont: xft:JetBrainsMono Nerd Font:style=Italic:size=12
URxvt*boldItalicfont: xft:JetBrainsMono Nerd Font:style=Italic Bold:size=12

URxvt*borderLess: false
URxvt*externalBorder: 0
URxvt*internalBorder: 4
URxvt*scrollBar: false
URxvt*saveLines: 5000
Urxvt*lineSpace: 1.2

URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt.keysym.Control-Shift-C: eval:selection_to_clipboard
URxvt.keysym.Control-Shift-V: eval:paste_clipboard


